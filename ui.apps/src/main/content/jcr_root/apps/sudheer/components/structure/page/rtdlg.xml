<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Rich Text Editor"
          sling:resourceType="cq/gui/components/authoring/dialog"
          helpPath="https://www.adobe.com/go/aem_cmp_text_v2">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                    cq:hideOnEdit="{Boolean}true"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/tabs"
                    maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <properties
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Properties"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured"/>
                    </properties>
                </items>
            </tabs>
            <columns
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                    margin="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <column
                            granite:class="cq-RichText-FixedColumn-column"
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <text
                                    cq:showOnCreate="{Boolean}true"
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                    fieldDescription="text.text-helptext"
                                    name="./text"
                                    useFixedInlineToolbar="{Boolean}true">
                                <rtePlugins jcr:primaryType="nt:unstructured">
                                    <format
                                            jcr:primaryType="nt:unstructured"
                                            features="*"/>
                                    <justify
                                            jcr:primaryType="nt:unstructured"
                                            features="*"/>
                                    <lists
                                            jcr:primaryType="nt:unstructured"
                                            features="*"/>
                                    <links
                                            jcr:primaryType="nt:unstructured"
                                            features="*"/>
                                    <edit
                                            jcr:primaryType="nt:unstructured"
                                            features="*"/>
                                    <paraformat
                                            jcr:primaryType="nt:unstructured"
                                            features="*">
                                        <formats jcr:primaryType="nt:unstructured">
                                            <default_p
                                                    jcr:primaryType="nt:unstructured"
                                                    description="Paragraph"
                                                    tag="p"/>
                                            <default_h2
                                                    jcr:primaryType="nt:unstructured"
                                                    description="Subhead"
                                                    tag="h2"/>
                                            <default_h3
                                                    jcr:primaryType="nt:unstructured"
                                                    description="Heading 3"
                                                    tag="h3"/>
                                            <default_h4
                                                    jcr:primaryType="nt:unstructured"
                                                    description="Heading 4"
                                                    tag="h4"/>
                                            <default_h5
                                                    jcr:primaryType="nt:unstructured"
                                                    description="Heading 5"
                                                    tag="h5"/>
                                            <default_blockquote
                                                    jcr:primaryType="nt:unstructured"
                                                    description="Quote"
                                                    tag="blockquote"/>
                                            <default_pre
                                                    jcr:primaryType="nt:unstructured"
                                                    description="Preformatted"
                                                    tag="pre"/>
                                        </formats>
                                    </paraformat>
                                    <styles
                                            jcr:primaryType="nt:unstructured"
                                            features="*">
                                        <styles jcr:primaryType="nt:unstructured">
                                            <label1 jcr:primaryType="nt:unstructured"
                                                    cssName="label label--primary"
                                                    text="Label 1"/>
                                            <label2 jcr:primaryType="nt:unstructured"
                                                    cssName="label label--secondary"
                                                    text="Label 2"/>
                                            <label3 jcr:primaryType="nt:unstructured"
                                                    cssName="label label--tertiary"
                                                    text="Label 3"/>
                                            <body1 jcr:primaryType="nt:unstructured"
                                                    cssName="body body--primary"
                                                    text="Body 1"/>
                                            <body2 jcr:primaryType="nt:unstructured"
                                                    cssName="body body--secondary"
                                                    text="Body 2"/>
                                        </styles>
                                    </styles>
                                    <misctools
                                            jcr:primaryType="nt:unstructured"
                                            features="*">
                                        <specialCharsConfig jcr:primaryType="nt:unstructured">
                                            <chars jcr:primaryType="nt:unstructured">
                                                <nbsp
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;nbsp;"/>
                                                <emDash
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;#45;"/>
                                                <registered
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;reg;"/>
                                                <trademark
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;#153;"/>
                                                <copyright
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;copy;"/>
                                                <grave-accent
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;#96;"/>
                                                <acute
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;acute;"/>
                                                <enDash
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;ndash;"/>
                                                <horizontal-ellipsis
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;hellip;"/>
                                                <dagger
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;dagger;"/>
                                                <double-dagger
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;Dagger;"/>
                                                <pound
                                                        jcr:primaryType="nt:unstructured"
                                                        description="Pound"
                                                        entity="Â£"/>
                                                <dollar
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;#36;"/>
                                                <euro
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;euro;"/>
                                                <yen
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;yen;"/>
                                                <currency
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;curren;"/>
                                                <faction-one-quarter
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;frac14"/>
                                                <fraction-one-half
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;frac12;"/>
                                                <fraction-three-quarters
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;frac34;"/>
                                                <superscript1
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;sup1;"/>
                                                <superscript2
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;sup2;"/>
                                                <superscript3
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;sup3;"/>
                                                <function
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;fnof;"/>
                                                <plus-or-minus
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;plusmn;"/>
                                                <multiplication
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;times;"/>
                                                <division
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;divide;"/>
                                                <degree
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;deg;"/>
                                                <micro
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;micro;"/>
                                                <lessthan-or-equalto
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;#8804;"/>
                                                <greaterthan-or-equalto
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;#8805;"/>
                                                <uppercase-alpha
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;Alpha;"/>
                                                <uppercase-beta
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;Beta;"/>
                                                <uppercase-gamma
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;Gamma;"/>
                                                <uppercase-delta
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;Delta;"/>
                                                <uppercase-epsilon
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;Epsilon;"/>
                                                <uppercase-zeta
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;Zeta;"/>
                                                <uppercase-eta
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;Eta;"/>
                                                <uppercase-theta
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;Theta;"/>
                                                <uppercase-iota
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;Iota;"/>
                                                <uppercase-kappa
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;Kappa;"/>
                                                <uppercase-lambda
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;Lambda;"/>
                                                <uppercase-mu
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;Mu;"/>
                                                <uppercase-nu
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;Nu;"/>
                                                <uppercase-xi
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;Xi;"/>
                                                <uppercase-omicron
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;Omicron;"/>
                                                <uppercase-pi
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;Pi;"/>
                                                <uppercase-rho
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;Rho;"/>
                                                <uppercase-sigma
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;Sigma;"/>
                                                <uppercase-tau
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;Tau;"/>
                                                <uppercase-upsilon
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;Upsilon;"/>
                                                <uppercase-phi
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;Phi;"/>
                                                <uppercase-chi
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;Chi;"/>
                                                <uppercase-psi
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;Psi;"/>
                                                <uppercase-omega
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;Omega;"/>
                                                <lowercase-alpha
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;alpha;"/>
                                                <lowercase-beta
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;beta;"/>
                                                <lowercase-gamma
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;gamma;"/>
                                                <lowercase-delta
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;delta;"/>
                                                <lowercase-epsilon
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;epsilon;"/>
                                                <lowercase-zeta
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;zeta;"/>
                                                <lowercase-eta
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;eta;"/>
                                                <lowercase-theta
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;theta;"/>
                                                <lowercase-iota
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;iota;"/>
                                                <lowercase-kappa
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;kappa;"/>
                                                <lowercase-lambda
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;lambda;"/>
                                                <lowercase-mu
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;mu;"/>
                                                <lowercase-nu
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;nu;"/>
                                                <lowercase-zi
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;xi;"/>
                                                <lowercase-omicron
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;omicron;"/>
                                                <lowercase-pi
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;pi;"/>
                                                <lowercase-rho
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;rho;"/>
                                                <lowercase-final-sigma
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;sigmaf;"/>
                                                <lowercase-sigma
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;sigma;"/>
                                                <lowercase-tau
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;tau;"/>
                                                <lowercase-upsilon
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;upsilon;"/>
                                                <lowercase-phi
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;phi;"/>
                                                <lowercase-chi
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;chi;"/>
                                                <lowercase-psi
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;psi;"/>
                                                <lowercase-omega
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;omega;"/>
                                                <greek-theta
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;thetasym;"/>
                                                <greek-upsilon-with-hook
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;upsih;"/>
                                                <greek-pi
                                                        jcr:primaryType="nt:unstructured"
                                                        entity="&amp;piv;"/>
                                            </chars>
                                        </specialCharsConfig>
                                    </misctools>
                                    <spellcheck
                                            jcr:primaryType="nt:unstructured"
                                            features="*"/>
                                    <undo
                                            jcr:primaryType="nt:unstructured"
                                            features="*"/>
                                    <subsuperscript
                                            jcr:primaryType="nt:unstructured"
                                            features="*"/>
                                </rtePlugins>
                                <uiSettings jcr:primaryType="nt:unstructured">
                                    <cui jcr:primaryType="nt:unstructured">
                                        <inline
                                                jcr:primaryType="nt:unstructured"
                                                toolbar="[edit#cut,edit#copy,edit#paste-default,edit#paste-plaintext,edit#paste-wordhtml,#paraformat,links#modifylink,format#bold,format#italic,format#underline,#justify,#lists,links#unlink,misctools#specialchars,misctools#sourceedit,fullscreen#start,image#imageProps,spellcheck#checktext,undo#undo,undo#redo,subsuperscript#subscript,subsuperscript#superscript,links#anchor,#styles]">
                                            <popovers jcr:primaryType="nt:unstructured">
                                                <justify
                                                        jcr:primaryType="nt:unstructured"
                                                        items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
                                                        ref="justify"/>
                                                <lists
                                                        jcr:primaryType="nt:unstructured"
                                                        items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
                                                        ref="lists"/>
                                                <paraformat
                                                        jcr:primaryType="nt:unstructured"
                                                        items="paraformat:getFormats:paraformat-pulldown"
                                                        ref="paraformat"/>
                                                <styles
                                                        jcr:primaryType="nt:unstructured"
                                                        items="styles:getStyles:styles-pulldown"
                                                        ref="styles"/>
                                            </popovers>
                                        </inline>
                                        <dialogFullScreen
                                                jcr:primaryType="nt:unstructured"
                                                toolbar="[edit#cut,edit#copy,edit#paste-default,edit#paste-plaintext,edit#paste-wordhtml,links#modifylink,format#bold,format#italic,format#underline,justify#justifyleft,justify#justifycenter,justify#justifyright,lists#unordered,lists#ordered,lists#outdent,lists#indent,links#unlink,table#createoredit,#paraformat,image#imageProps,misctools#specialchars,misctools#sourceedit,spellcheck#checktext,undo#undo,undo#redo,subsuperscript#subscript,subsuperscript#superscript,misctools#specialchars#general,links#anchor,#styles]">
                                            <popovers jcr:primaryType="nt:unstructured">
                                                <paraformat
                                                        jcr:primaryType="nt:unstructured"
                                                        items="paraformat:getFormats:paraformat-pulldown"
                                                        ref="paraformat"/>
                                                <styles
                                                    jcr:primaryType="nt:unstructured"
                                                    items="styles:getStyles:styles-pulldown"
                                                    ref="styles"/>
                                            </popovers>
                                        </dialogFullScreen>
                                        
                                    </cui>
                                </uiSettings>
                            </text>
                        </items>
                    </column>
                </items>
            </columns>
        </items>
    </content>
</jcr:root>
